{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "env": {
        "MOCK_ALL": "true",
        "IS_DEV": "true"
      },
      "name": "Debug Application - avito with mocks",
      "args": ["${workspaceFolder}/apps/avito/src/main.ts"],
      "runtimeArgs": [
        "--nolazy",
        "-r",
        "ts-node/register",
        "-r",
        "tsconfig-paths/register"
      ],
      "sourceMaps": true,
      "cwd": "${workspaceRoot}",
      "outputCapture": "std"
