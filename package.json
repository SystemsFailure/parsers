{
  "name": "crm-parsers",
  "version": "1.2.4",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "prod:start": "./scripts/start-parsers.sh --",
    "prod:stop": "./scripts/stop-parsers.sh",
    "start": "nest start",
    "start:dev:mock": "npx cross-env MOCK_ALL=true IS_DEV=true nest start --watch",
    "start:dev:local": "npx cross-env MOCK_ALL=true nest start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:avito": "jest apps/avito",
    "test:avito:parsers": "jest apps/avito/src/parser-v1/parsers",
    "test:avito:extractor": "jest apps/avito/src/parser-v1/extractor",
    "test:cian": "jest apps/cian",
    "test:domria": "jest apps/domria",
    "test:olx": "jest apps/olx",
    "test:youla": "jest apps/youla",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.583.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/microservices": "^10.3.8",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.3.1",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.4",
    "axios": "^1.6.8",
    "axios-https-proxy-fix": "^0.17.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "crypto": "^1.0.1",
    "dom-parser": "^0.1.6",
    "install": "^0.13.0",
    "nest-winston": "^1.10.0",
    "nestjs-s3": "^2.0.1",
    "playwright": "^1.44.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "tesseract.js": "^5.1.0",
    "typescript-retry-decorator": "^2.4.2",
    "urldecode": "^1.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/dom-parser": "^0.1.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.17.1",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "lodash": "^4.17.21",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/",
      "<rootDir>/@utils/",
      "<rootDir>/@types/",
      "<rootDir>/@constants/"
    ],
    "moduleNameMapper": {
      "@constants": "<rootDir>/@constants",
      "@types": "<rootDir>/@types",
      "@utils": "<rootDir>/@utils",
      "^@app/playwright(|/.*)$": "<rootDir>/libs/playwright/src/$1",
      "^@app/proxy-servers(|/.*)$": "<rootDir>/libs/proxy-servers/src/$1",
      "^@app/rent-object(|/.*)$": "<rootDir>/libs/rent-object/src/$1",
      "^@app/rent-object-checker(|/.*)$": "<rootDir>/libs/rent-object-checker/src/$1",
      "^@app/flags(|/.*)$": "<rootDir>/libs/flags/src/$1",
      "^@app/job-runner(|/.*)$": "<rootDir>/libs/job-runner/src/$1",
      "^@app/geocord(|/.*)$": "<rootDir>/libs/geocord/src/$1",
      "^@app/logger(|/.*)$": "<rootDir>/libs/logger/src/$1",
      "^@app/rent-object-photos(|/.*)$": "<rootDir>/libs/rent-object-photos/src/$1"
    }
  }
}
